{
  "name": "portifolio",
  "homepage": "http://artutra.github.io/portifolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.62",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "styled-components": "^5.1.1",
    "tailwindcss": "^1.4.6",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "start": "run-p watch:tailwind start:react",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build:tailwind": "postcss src/assets/css/tailwind.css -o src/assets/css/tailwind.generated.css",
    "watch:tailwind": "postcss -w src/assets/css/tailwind.css -o src/assets/css/tailwind.generated.css",
    "start:react": "react-scripts start",
    "prebuild": "npm run build:tailwind"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^3.1.0"
  }
}
